CREATE TABLE Reservation (
    id TEXT PRIMARY KEY,
    userId TEXT,
    chargeHubId TEXT NOT NULL,
    startTime TEXT NOT NULL,
    endTime TEXT NOT NULL,
    FOREIGN KEY (userId) REFERENCES User(id),
    FOREIGN KEY (chargeHubId) REFERENCES ChargeHub(id)
);

getAllReservations:
SELECT * FROM Reservation;

getReservationById:
SELECT * FROM Reservation WHERE id = ?;

updateReservation:
UPDATE Reservation SET userId = ?, chargeHubId = ?, startTime = ?, endTime = ? WHERE id = ?;

deleteReservation:
DELETE FROM Reservation WHERE id = ?;

insertReservation:
INSERT INTO Reservation (userId, chargeHubId, startTime, endTime) VALUES (?, ?, ?, ?);