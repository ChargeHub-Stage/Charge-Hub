CREATE TABLE Level (
    id TEXT PRIMARY KEY,
    level INTEGER NOT NULL,
    requiredPoints INTEGER NOT NULL
);

getAllLevels:
SELECT * FROM Level;

getLevelById:
SELECT * FROM Level WHERE id = ?;

updateLevel:
UPDATE Level SET level = ?, requiredPoints = ? WHERE id = ?;

deleteLevel:
DELETE FROM Level WHERE id = ?;

insertLevel:
INSERT INTO Level (level, requiredPoints) VALUES (?, ?);