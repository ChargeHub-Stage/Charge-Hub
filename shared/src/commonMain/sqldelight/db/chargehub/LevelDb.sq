CREATE TABLE Level (
    id INTEGER PRIMARY KEY,
    level INTEGER NOT NULL,
    pointsToNextLevel INTEGER NOT NULL,
    requiredPoints INTEGER NOT NULL
);

getAllLevels:
SELECT * FROM Level;

getLevelById:
SELECT * FROM Level WHERE id = ?;

updateLevel:
UPDATE Level SET level = ?, pointsToNextLevel = ?, requiredPoints = ? WHERE id = ?;

deleteLevel:
DELETE FROM Level WHERE id = ?;

insertLevel:
INSERT INTO Level (level, pointsToNextLevel, requiredPoints) VALUES (?, ?, ?);