CREATE TABLE Car (
    id TEXT PRIMARY KEY,
    userId TEXT NOT NULL,
    plate TEXT NOT NULL,
    brand TEXT NOT NULL,
    FOREIGN KEY (userId) REFERENCES User(id)
);

getAllCars:
SELECT * FROM Car;

getCarById:
SELECT * FROM Car WHERE id = ?;

updateCar:
UPDATE Car SET userId = ?, plate = ?, brand = ? WHERE id = ?;

deleteCar:
DELETE FROM Car WHERE id = ?;

insertCar:
INSERT INTO Car (userId, plate, brand) VALUES (?, ?, ?);